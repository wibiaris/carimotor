<%= simple_form_for @motor, html: { multipart: true } do |f| %>
	<% if @motor.errors.any? %>
	<div id="errors">
		<h2>
			<%= pluralize(@motor.errors.count, "error") %>
		</h2>
		<ul>
			<% @motor.errors.full_messages.each do |msg| %>
				<li><%= msg %></li>
			<% end %>

		</ul>
	</div>
	<% end %>
	<div class="form-group">
		<%= f.input :platno, input_html: { class: 'form-control'} %>
	</div>

	<div class="form-group">
		
		<%= f.input :merk_id, input_html: { class: 'form-control'} %>
	</div>

	<div class="form-group">
		
		<%= f.input :type_id, input_html: { class: 'form-control'} %>
	</div>

	<div class="form-group">
		
		<%= f.input :harga, input_html: { class: 'form-control'} %>
	</div>

	<div class="form-group">
		
		<%= f.input :warna, input_html: { class: 'form-control'} %>
	</div>

	<div class="form-group">

		<%= f.input :tahun, input_html: { class: 'form-control'} %>
		
	</div>
	<div class="form-group">
		<%= f.input :image, input_html: { clas: 'form-control' } %>
	</div>
	<%= f.button :submit, class: "btn btn-primary" %>

	<% end %>